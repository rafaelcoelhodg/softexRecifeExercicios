#Exercício realizado em Linguagem C - Ordenação em Vetor por Bubblesort

#include<stdio.h>

int main(){
	int vetorA[10];
	int i, j, troca;
	for(i=0; i<10; i++){
		printf("Digite o %d elemento: ", i);
		scanf("%d", &vetorA[i]);
	}
	for(i=0; i<9; i++){
		for(j=i+1; j<10; j++){
			if(vetorA[i] > vetorA[j]){
				troca = vetorA[i];
				vetorA[i] = vetorA[j];
				vetorA[j] = troca;
			}
		}
	}
	printf("\n Vetor ordenado: \n");
	for(i=0; i<10; i++){
		printf("%d - ", vetorA[i]);
	}
	return(0);
}
